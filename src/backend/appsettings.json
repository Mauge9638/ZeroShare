{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DbConnection": "Host=<DATABASE_URL>;Database=<DATABASE>;Username=<DATABASE_USER>;Password=<PASSWORD>;SSL Mode=Require;Trust Server Certificate=true;"
  },
  "SnippetCleanup": {
    "InactivityRetentionDays": 60,
    "IntervalCleanupMinutes": 5
  }
}
